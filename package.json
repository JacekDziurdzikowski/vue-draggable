{
  "name": "vue-draggable",
  "version": "1.0.6",
  "description": "Vuejs 2.* directive for drag and drop",
  "main": "lib/vue-draggable.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack-dev-server --progress --colors --watch --env server --config ./webpack.config.js",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vivify-Ideas/vue-draggable"
  },
  "bugs": {
    "url": "https://github.com/Vivify-Ideas/vue-draggable/issues"
  },
  "keywords": [
    "vuejs",
    "html5",
    "drag",
    "drop",
    "draggable"
  ],
  "author": "Nikola Spalevic",
  "license": "MIT",
  "homepage": "https://github.com/Vivify-Ideas/vue-draggable#readme",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "eslint": "^5.1.0",
    "eslint-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^11.11.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "yargs": "^12.0.1"
  },
  "dependencies": {}
}
